: intro
	clear

	va := 10
	vb := 10
	vc := text_title_1
	draw_text

	v9 := va

	va := 60
	sleep

	vd := 5

	va := v9

	loop
		ve := va
		va := 30
		sleep
		va := ve

		vc := 42 #'I'
		_draw_font_char

		i := long audio_sweep
		audio_play_sync

		vd += -1
	if vd > 0 then again

	va := v9
	vb := 13
	vc := text_title_2
	draw_text

	va := 120
	sleep

	clear

	va := 10
	vb := 30
	vc := text_title_3
	draw_text

	va := 120
	sleep

	clear
	loop
	again
	return
